{
  "name": "dan-and-ches",
  "version": "0.1.0",
  "description": "Web application for Dan & Ches' wedding",
  "repository": "https://github.com/dan-j/dan-and-ches",
  "engines": {
    "node": "7.1.0",
    "npm": "3.10.6"
  },
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build",
    "build-server": "babel -d ./build server.js -s && babel -d ./build config.js -s && babel -d ./build/server server -s",
    "build-client": "webpack -p --config webpack.config.js",
    "build": "npm run clean && npm run build-client && npm run build-server",
    "postinstall": "npm run build",
    "start": "node build/server.js",
    "start:clean": "npm run build && npm run start",
    "dev": "NODE_ENV=development babel-node $NODE_DEBUG_OPTION server.js",
    "test": "jest",
    "lint": "esw --color --ext .js --ext .jsx server client webpack.*.config*",
    "lint:watch": "npm run lint -- --watch"
  },
  "author": "dan-j",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "chokidar": "^1.6.1",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.1.0",
    "mongoose": "^4.6.8",
    "morgan": "^1.7.0",
    "node-sass": "^3.13.0",
    "passport": "^0.3.2",
    "passport-json": "^1.2.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "react": "^15.4.0",
    "react-bootstrap": "^0.30.7",
    "react-count-down": "^1.0.3",
    "react-dom": "^15.4.0",
    "react-fontawesome": "^1.5.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.0",
    "react-slick": "^0.14.5",
    "reactable": "^0.14.1",
    "sass-loader": "^4.0.2",
    "slick-carousel": "^1.6.0",
    "style-loader": "^0.13.1",
    "timediff": "^1.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "whatwg-fetch": "^2.0.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-preset-airbnb": "^2.1.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "eslint-watch": "^2.1.14",
    "jest": "^17.0.3",
    "react-test-renderer": "^15.4.0"
  }
}
